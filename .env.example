# Trac MCP Server - Environment Variables
# Copy to .env and fill in your values.

# Required: Trac instance URL
TRAC_URL=https://trac.example.com/trac

# Required: Trac credentials
TRAC_USERNAME=your-username
TRAC_PASSWORD=your-password

# Optional: Skip SSL certificate verification (default: false)
# TRAC_INSECURE=false

# Optional: Max parallel XML-RPC requests, 1-100 (default: 5)
# TRAC_MAX_PARALLEL_REQUESTS=5

# Optional: Enable debug logging (default: false)
# TRAC_DEBUG=false
